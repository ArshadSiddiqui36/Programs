#include <iostream>

using namespace std;

int main()
{
    int i, n, m;
    cout<<"Display the prime number in given a range by user.....\n";
    cout<<"Enter Minimum Number : ";
    cin>>n;
    cout<<"Enter Maximum Number : ";
    cin>>m;
    
    while(n <= m){
        i = 2;
        while(i<n){
           if(n%i==0){
               break;
            }
            i=i+1;
        }
        if(n==i){
            cout<<n;
        }
        n=n+1;
    }

    return 0;
}
